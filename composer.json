{
    "name" : "stackinstance/mailer-bundle",
    "description" : "Stack Instance Mailer",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "Ray Kootstra",
        "email" : "r.kootstra@stackinstance.com"
    }],
    "keywords" : [
        "mailer bundle"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": ">=5.5.0",
        "swiftmailer/swiftmailer": "5.4.*"
    },
    "repositories" : [{
        "type" : "vcs",
        "url" : "https://github.com/stackinstance/mailer-bundle"
    }],
    "autoload": {
        "psr-0": { "StackInstance\\MailerBundle": "" }
    },
    "target-dir": "StackInstance/MailerBundle",
    "version": "2.0.1",
    "minimum-stability": "stable",
    "prefer-stable": true
}
